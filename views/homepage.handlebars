{{#each blogs as |blog|}}
<div class="container">
  <div class="card">
  <div class="card-body">
    <h5 class="card-title">{{blog.title}}</h5>
    <p class="card-text">{{blog.text}}</p>
    <p class="card-text">Author: {{blog.user.username}}</p>
    <a href="#" class="btn btn-primary">Comment</a>
  </div>
</div>
</div>
{{/each}}